schema.module.build.auto.upgrade=true

theme.css.fast.load=false
theme.css.fast.load.check.request.parameter=true
theme.images.fast.load=false
theme.images.fast.load.check.request.parameter=true

javascript.fast.load=true
javascript.log.enabled=false

layout.template.cache.enabled=false

browser.launcher.url=

combo.check.timestamp=true

minifier.enabled=false

module.framework.properties.initial.system.check.enabled=true
module.framework.properties.osgi.console=localhost:11311

# default is false: reload JSP file changes w/o redeploy bundle
# work.dir.override.enabled=true

com.liferay.portal.servlet.filters.cache.CacheFilter=false
com.liferay.portal.servlet.filters.etag.ETagFilter=false
com.liferay.portal.servlet.filters.header.HeaderFilter=false
com.liferay.portal.servlet.filters.themepreview.ThemePreviewFilter=true



# ---------------------------



# Set this to true to undeploy a plugin before deploying a new version. This
# property will only be used if the property "hot.undeploy.enabled" is set
# to true.
#hot.undeploy.on.redeploy=true

#Company (Actually the name of the site that is generated by liferay)
#company.default.name=SW360

# This sets the default web id. Omniadmin users must belong to the company
# with this web id.
#company.default.web.id=sw360.org

# This sets the default home URL of the portal.
#company.default.home.url=/web/guest

## Default Admin
# Set the default admin password.
#default.admin.password=<%= @liferay_admin_password %>

# Set the default admin screen name prefix.
#default.admin.screen.name=<%= @liferay_admin_name %>

# Set the default admin email address prefix.
#default.admin.email.address.prefix=<%= @liferay_admin_name %>

# Set the default admin first name.
#default.admin.first.name=Setup Administrator

# Set the default admin last name.
#default.admin.last.name=

# Default landing page when logged-in
#default.landing.page.path=/group/guest/home

# Specify the site name that will default to the company's virtual host.
# If the specified site has a virtual host, then that will take precedence.
# If it does not, then it will use the company's virtual host. This property
# is useful to remove "/web/guest" (or any other site) from the default URL.
# For example, if this property is not set, then the default URL may be
# http://localhost:8080/web/guest/home. If this property is set, then the
# default URL may be http://localhost:8080/home.
#virtual.hosts.default.site.name=SW360

# Set this property to true if the Setup Wizard should be displayed the
# first the portal is started.
#setup.wizard.enabled=false

# Disable asking for the security question
#users.reminder.queries.enabled=false

# To turn off the terms and conditions
#terms.of.use.required=false

#schema.run.minimal=true

# User creation preferences
#login.create.account.allow.custom.password=false
#recaptcha.enabled=false
#company.security.send.password=false
#company.security.send.password.reset.link=false
# Do not check password
#auth.pipeline.enable.liferay.check=false

#
# Specify a LAR file that can be used to create the guest public layouts.
# If this property is set, the previous layout properties will be ignored.
#

#default.guest.public.layouts.lar=/home/siemagrant/source/frontend/configuration/public_pages_sw360.lar




#Restric user creativity
#layout.user.public.layouts.enabled=true
#layout.user.public.layouts.modifiable=true
#layout.user.public.layouts.auto.create=true

#tune to improve performance
#theme.css.fast.load=true
#javascript.fast.load=true
#layout.parallel.render.enable=false
#hot.deploy.hook.custom.jsp.verification.enabled=true
